<thlogic xmlns:th="http://www.thymeleaf.org">

    <attr
         sel="#headId"
         th:replace="~{fragments/head :: headFragment}"
    />

    <attr
            sel="#play"
            th:href="@{__${T(com.imtiaz.util.GameMappings).PLAY}__}"
            th:text="Play"
    />

    <attr
            sel="#footerId"
            th:replace="~{fragments/footer :: footer}"
    />

</thlogic>

